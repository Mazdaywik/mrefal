$MODULE InOut;

$IMPORT MRefalRTS;

$ENTRY WriteLine {
	e.Line = <MRefalRTS::Prout e.Line>;
}

$ENTRY WriteLine-T {
	e.Line =
		<MRefalRTS::Prout e.Line>
		e.Line;
}

$ENTRY ReadLine {
	= <MRefalRTS::Card>;
}

$ENTRY WriteText {
	(e.Line) e.Tail =
		<WriteLine e.Line>
		<WriteText e.Tail>;

	= ;
}

$ENTRY WriteText-T {
	(e.Line) e.Tail =
		( <WriteLine-T e.Line> )
		<WriteText-T e.Tail>;

	= ;
}

$END InOut.
