$ENTRY OneOf {
	t.X (e.1 t.X e.2) = T;
	t.X (e.1) = F;
}

$ENTRY FirstOneOf {
	(e.Pat) t.First e.Tail =
		<OneOf t.First (e.Pat)> t.First e.Tail;
}

$ENTRY UpAlph {
	= 'ABCDEFGHIJKLMNOPQRSTUVWXYZ';
}

$ENTRY LoAlph {
	= 'abcdefghijklmnopqrstuvwxyz';
}

$ENTRY Digits {
	= '01234567890';
}

