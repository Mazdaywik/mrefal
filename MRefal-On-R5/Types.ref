$ENTRY CharType {
	s.Char e.Tail = <SwCharType s.Char 'U' <OneOf s.Char (<UpAlph>)> e.Tail>;
	= '*';
};

SwCharType {
	s.Char s.Type T e.Tail
		= s.Type s.Char e.Tail;
	s.Char 'U' F e.Tail
		= <SwCharType s.Char 'l' <OneOf s.Char (<UpAlph>)> e.Tail>;
	s.Char 'l' F e.Tail
		= <SwCharType s.Char 'D' <OneOf s.Char (<UpAlph>)> e.Tail>;
	s.Char 'D' F e.Tail = 'O';
};
