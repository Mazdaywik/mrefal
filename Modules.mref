$MODULE Modules;

$IMPORT MModules;

$ENTRY LookupModule-T
  t.hContext e.ModuleName =
    <MModules::LookupModule-T t.hContext e.ModuleName>;

$ENTRY SymName
  t.hContext e.ModuleName =
    <MModules::SymName t.hContext e.ModuleName>;

$ENTRY OutName
  t.hContext e.ModuleName =
    <MModules::OutName t.hContext e.ModuleName>;

$ENTRY HomeDir = <MModules::HomeDir>;

$ENTRY CanonicalModuleName
  e.ModuleName =
    <MModules::CanonicalModuleName e.ModuleName>;

$END Modules.
