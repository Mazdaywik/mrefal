$MODULE Modules;

$IMPORT Refal5;

$ENTRY CanonicalModuleName {
	s.Initial e.Tail =
		<Refal5::Upper s.Initial> <Refal5::Lower e.Tail>;
}

$ENTRY SymName {
	e.ModName = 'Defs\\' e.ModName '.rsym';
}

$ENTRY OutName {
	e.ModName = 'ROut\\' e.ModName '.rout';
}

$ENTRY SrcName {
	e.ModName = e.ModName '.mref';
}

$END Modules.
