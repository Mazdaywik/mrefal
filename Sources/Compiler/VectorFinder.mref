$PROGRAM VectorFinder;

$IMPORT MLambda;

$IMPORT FindVectors::FileList;
$IMPORT FindVectors::Scanner;
$IMPORT FindVectors::Printer;

$ENTRY Go =
  <FindVectors::Printer::Print
    <MLambda::Map
      <MLambda::Composite
        & MLambda.UnBracket
        & FindVectors.Scanner.ScanFile
      >
      <FindVectors::FileList::GetList>
    >
  >;

$END VectorFinder.
