
/*
  Генератор лексических анализаторов.
*/

//FROM LibraryEx
$EXTERN ArgList;

//FROM Library
$EXTERN WriteLine;

//FROM LexGen_Generator
$EXTERN Generate;

//FROM sUtilitiesBanner
$EXTERN TypeBanner;

Main {
  (e.ProgName) (e.File) =
    <Generate e.File>;

  (e.ProgName) e.Other = <WriteLine 'Command line error'>;
}

$ENTRY Go {
  =
    <TypeBanner 'LexGen'>
    <Main <ArgList>>;
}