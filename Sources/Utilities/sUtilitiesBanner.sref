//FROM Library
$EXTERN WriteLine;

$FORWARD VerNumber;

BannerStr {
  e.ProgName =
    e.ProgName ' from MRefal Utilities Kit ' <VerNumber> ' (c) Mazdaywik 2007-2010';
}

$ENTRY TypeBanner {
  e.ProgName =
    <WriteLine <BannerStr e.ProgName>>;
}

//{{GEN:VERSION

VerNumber { = '0.1.120'; }

//}}GEN
